<form method="post" action="{{ path('jeux_delete', {'id': jeux.id}) }}" onsubmit="return confirm('Confirmer la suppression ?');">
    <input type="hidden" name="_method" value="Supprimer">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ jeux.id) }}">
    <button class="btn">Supprimer</button>
</form>
